{
	"name": "floatplane-plex-downloader",
	"version": "5.5.0",
	"private": true,
	"type": "module",
	"scripts": {
		"start": "tsc && node ./dist/float.js",
		"startHeadless": "tsc && node ./dist/float.js --headless",
		"build": "esbuild ./src/float.ts --bundle --platform=node --outfile=dist/float.cjs && pkg ./dist/float.cjs --out-path=./build -t latest-linux,latest-mac,latest-win --compress GZip",
		"checkLint": "eslint ./src",
		"lint": "eslint ./src --fix",
		"sanity": "npm run checkLint && npm run testESM && npm run testBinaryWin",
		"testESM": "tsc && node ./dist/float.js --sanityCheck",
		"testBinaryLinux": "npm run build && ./build/float-linux --sanityCheck",
		"testBinaryWin": "npm run build && cd ./build && float-win.exe --sanityCheck"
	},
	"dependencies": {
		"@ctrl/plex": "^1.5.3",
		"@inrixia/db": "2.0.2",
		"@inrixia/helpers": "^2.0.7",
		"chalk-template": "^0.4.0",
		"default-import": "^1.1.5",
		"dotenv": "^16.0.3",
		"ffbinaries": "^1.1.4",
		"floatplane": "^4.2.2",
		"html-to-text": "^8.2.1",
		"json5": "^2.2.1",
		"multi-progress-bars": "^4.3.1",
		"process.argv": "^0.6.0",
		"prompts": "^2.4.2",
		"sanitize-filename": "^1.6.3",
		"semver": "^7.3.8",
		"tough-cookie": "^4.1.2",
		"tough-cookie-file-store": "^2.0.3"
	},
	"devDependencies": {
		"@types/html-to-text": "^8.1.1",
		"@types/multi-progress": "^2.0.3",
		"@types/prompts": "^2.4.1",
		"@types/semver": "^7.3.12",
		"@types/tough-cookie-file-store": "^2.0.1",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"esbuild": "^0.15.12",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"pkg": "^5.8.0",
		"typescript": "^4.8.4"
	}
}
