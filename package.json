{
  "name": "floatplane-plex-downloader",
  "version": "4.6.3",
  "private": true,
  "scripts": {
    "prep": "npm install && cd ./src/submodules/plex && npm run build",
    "start": "tsc && node ./dist/float.js",
    "build": "pkg ./dist/float.js --out-path=./build -t node14-win-x64",
    "test": "jest"
  },
  "bin": {
    "Floatplane-PlexDownloader": "float.js"
  },
  "dependencies": {
    "@ctrl/plex": "file:src/submodules/plex",
    "@inrixia/db": "^1.7.0",
    "@inrixia/helpers": "^1.15.1",
    "@types/prompts": "^2.0.9",
    "floatplane": "^2.1.0",
    "got": "^11.8.1",
    "multi-progress": "^4.0.0",
    "npm": "^7.3.0",
    "prompts": "^2.4.0",
    "sanitize-filename": "^1.6.3",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.2"
  },
  "pkg": {
    "scripts": "./float.js",
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  },
  "devDependencies": {
    "@types/multi-progress": "^2.0.3",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "jest": "^26.4.2",
    "typescript": "^4.1.3"
  }
}
