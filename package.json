{
  "name": "Floatplane-PlexDownloader",
  "version": "4.6.3",
  "private": true,
  "scripts": {
    "start": "node float.js pause",
    "build": "pkg . --out-path=./build -t node10-linux-x64,node10-macos-x64,node10-win-x64",
    "release": "./release.sh"
  },
  "bin": "./float.js",
  "dependencies": {
    "@inrixia/db": "^1.0.1",
    "@inrixia/helpers": "^1.7.7",
    "floatplane": "^1.1.1",
    "got": "^11.6.0",
    "multi-progress": "^3.0.0",
    "plex-api": "^5.3.1",
    "prompts": "^2.3.2"
  },
  "pkg": {
    "scripts": "./float.js",
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  }
}
