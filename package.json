{
  "name": "floatplane-plex-downloader",
  "version": "4.6.3",
  "private": true,
  "scripts": {
    "prep": "npm install && npm run build",
    "start": "tsc && node ./dist/float.js",
    "build": "tsc && pkg ./dist/float.js --out-path=./build -t latest-linux,latest-mac,latest-win"
  },
  "bin": {
    "Floatplane-PlexDownloader": "float.js"
  },
  "dependencies": {
    "@ctrl/plex": "^1.2.0",
    "@inrixia/db": "^1.7.0",
    "@inrixia/helpers": "^1.15.1",
    "ffbinaries": "^1.1.4",
    "floatplane": "^2.1.6",
    "got": "^11.8.2",
    "multi-progress-bars": "^3.2.2",
    "prompts": "^2.4.0",
    "sanitize-filename": "^1.6.3",
    "tough-cookie": "^4.0.0",
    "tough-cookie-file-store": "^2.0.2"
  },
  "pkg": {
    "scripts": "./float.js",
    "targets": [
      "node10-linux-x64",
      "node10-macos-x64",
      "node10-win-x64"
    ]
  },
  "devDependencies": {
    "@types/multi-progress": "^2.0.3",
    "@types/prompts": "^2.0.9",
    "@types/tough-cookie-file-store": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "pkg": "^4.4.9",
    "typescript": "^4.2.3"
  }
}
